
<div class="row-fluid">
	<div class="media pull-right">
	  	<%if (Model.image) {%>
	    <img class="media-object" src="/img/<%Print(Model.image);%>">
	    <%}%>
	</div>
</div>
<div class="row-fluid desc">
  	<div class="media pull-right">
	    <h4 class="media-heading"><%Print(Model.name);%></h4>
		<%if (Model.desc) {%>		
			<p><%Print(Model.desc);%></p>
		<%}%>
  	</div>		
  </div>	